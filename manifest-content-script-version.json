{
  "name": "Skip-Ad",
  "version": "1.0.0",
  "description": "A button that mutes and speeds up youtube ads",
  "manifest_version": 3,
  "action": {
    "default-title": "Skipper"
  },
  "content_scripts": [{
    "matches": ["https://www.youtube.com/*"],
    "js": ["script.js"]
  }],
  "web_accessible_resources": [{
    "resources": ["template.html", "font-awesome.min.css"],
    "matches": ["<all_urls>"]
  }]
}