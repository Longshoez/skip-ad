{
  "name": "SpeedAds",
  "version": "2.0.0",
  "description": "A button that mutes and speeds up youtube ads",
  "manifest_version": 3,
  "icons": {
    "16": "./icons/icon16.png",
    "32": "./icons/icon32.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "button-keymap.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}
